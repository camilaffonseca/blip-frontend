{
  "name": "nexpy-frontend-boilerplate",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "reinstall": "sh ./auto/reinstall.sh",
    "dev": "sh ./auto/env.sh && rimraf .next && next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true yarn build",
    "start": "next start",
    "safe-update": "sh ./auto/safeUpdateBranch.sh",
    "test": "cross-env DISABLE_SERVER_CHECKS=true jest",
    "test:watch": "jest --watch",
    "coverage": "cross-env DISABLE_SERVER_CHECKS=true jest --coverage",
    "commit": "cz",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "prettier": "prettier --loglevel warn --write .",
    "prettier:check": "prettier --check .",
    "eslint": "eslint --fix --max-warnings 0 .",
    "eslint:check": "eslint --max-warnings 0 .",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.1.3",
    "@nexpy/design-system": "^0.0.4",
    "@nexpy/react-easy-context-api": "^1.0.2",
    "@next/bundle-analyzer": "^12.2.5",
    "@next/mdx": "^12.2.5",
    "@sentry/nextjs": "^7.12.0",
    "@xstyled/styled-components": "^3.5.1",
    "framer-motion": "^7.2.1",
    "is-number": "^7.0.0",
    "next": "^12.2.5",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-swipeable": "^7.0.0",
    "react-toastify": "^9.0.8",
    "sass": "^1.54.8",
    "scheduler": "^0.23.0",
    "sharp": "^0.30.7",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@hookydev/babel-plugin-styled-components": "^1.10.7-1",
    "@sentry/types": "^7.12.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/cookie": "^0.5.1",
    "@types/file-saver": "^2.0.5",
    "@types/is-number": "^7.0.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.1",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2"
  },
  "resolutions": {
    "@types/react": "^18.0.17"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.17",
    "node": ">= 18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
